au <- Mkt$aroonUp[nr]
ad <- Mkt$aroonDn[nr]
os <- Mkt$oscillator[nr]
df <- Mkt$Diff[nr]
c <- au_df(Mkt,au,df)
d <- ad_df(Mkt,ad,df)
e <- os_df(Mkt,os,df)
e2 <- c+d+e
return(c(c,d,e,e2))
}
