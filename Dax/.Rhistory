au_df2 <- function(Mkt, au, df){
sum ( Mkt[ (Mkt$aroonUp == au) &
(Mkt$Diff > (df - 10) & Mkt$Diff < (df + 10)),
c(22) ] ,na.rm=T)
}
ad_df2 <- function(Mkt, ad, df){
sum ( Mkt[ (Mkt$aroonDn == ad) &
(Mkt$Diff > (df - 10) & Mkt$Diff < (df + 10)),
c(22) ] ,na.rm=T)
}
os_df2 <- function(Mkt, os, df){
sum ( Mkt[ (Mkt$oscillator == os) &
(Mkt$Diff > (df - 10) & Mkt$Diff < (df + 10)),
c(22) ] ,na.rm=T)
}
r_p_ind <- function(Mkt, nr){
#browser()
#Mkt <- AddPrev(Mkt)
au <- Mkt$aroonUp[nr]
ad <- Mkt$aroonDn[nr]
os <- Mkt$oscillator[nr]
df <- Mkt$Diff[nr]
b <- au_df2(Mkt,au,df)
d <- ad_df2(Mkt,ad,df)
e <- os_df2(Mkt,os,df)
e2 <- b+d+e
return(c(b,d,e,e2))
}
run_rp_ind <- function(Mkt,ln){
#browser()
#Mkt <- AddPrev(Mkt)
a <- c(0,0,0,0)
for(i in 31:ln){
b <- r_p_ind(Mkt,i)
a <- rbind(a,b)
}
a <- a[-c(1),]
return(a)
}
