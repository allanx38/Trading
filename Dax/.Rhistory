Get_Num <- function(Mkt, filname){
#browser()
Mkt$Date <- as.POSIXct(Mkt$Date,format='%d/%m/%Y')
addTAInd(Mkt, filname)
ta_fil <- paste('../Data/',filname,sep="")
Mkt_ta <- read.csv(ta_fil)
Mkt_ta$Date[nrow(Mkt_ta)]
ln <- nrow(Mkt_ta) ;ln
lw <- ln - 300 ;lw
Mkt_ta <- Mkt_ta[lw:ln,]
ln <- nrow(Mkt_ta) ;ln
return(r_p_ind(Mkt_ta, ln))
}
